		    <body>
		  	<ST>
		  	  <xhtml xmlns="http://www.w3.org/1999/xhtml">
		  		(****** Init_Grafcet ******)
		  		IF Init THEN
		  			Step_1 := true;
		  			actStep_1 := false;
		  			deactStep_1 := false;
		  			Step_5 := false;
		  			actStep_5 := false;
		  			deactStep_5 := false;
		  			Step_4 := false;
		  			actStep_4 := false;
		  			deactStep_4 := false;
		  			Step_3 := false;
		  			actStep_3 := false;
		  			deactStep_3 := false;
		  		END_IF
		  		(****** End_Grafcet ******)
		  		IF End THEN
		  			Step_1 := false;
		  			actStep_1 := false;
		  			deactStep_1 := false;
		  			Step_5 := false;
		  			actStep_5 := false;
		  			deactStep_5 := false;
		  			Step_4 := false;
		  			actStep_4 := false;
		  			deactStep_4 := false;
		  			Step_3 := false;
		  			actStep_3 := false;
		  			deactStep_3 := false;
		  		END_IF
		  		(****** Program ******)
		  		IF (Enable) THEN
		  			TranLoops := (0);
		  			StableState := true;
		  			WHILE ((StableState)AND(TranLoops&lt;NumTran)) DO
		  			StableState := false;
		  			TranLoops := (TranLoops+1);
		  			(****** Transition_Part ******)
		  			IF ((INIT=FALSE)AND(END=FALSE)) THEN
		  				(****** Enable_Transitions ******)
		  				T_3 := (Step_1);
		  				T_1 := (Step_5);
		  				T_6 := (Step_3);
		  				T_5 := (Step_4);
		  				T_4 := (Step_1);
		  				(****** T_3 ******)
		  				IF ((DI_3)AND(T_3)) THEN
		  					StableState := true;
		  					(****** deactivate_presteps ******)
		  					Step_1 := false;
		  					actStep_1 := false;
		  					deactStep_1 := true;
		  					(****** activate_poststeps ******)
		  					Step_5 := true;
		  					actStep_5 := true;
		  					deactStep_5 := false;
		  				END_IF
		  				(****** T_1 ******)
		  				IF ((DI_1 AND DI_2)AND(T_1)) THEN
		  					StableState := true;
		  					(****** deactivate_presteps ******)
		  					Step_5 := false;
		  					actStep_5 := false;
		  					deactStep_5 := true;
		  					(****** activate_poststeps ******)
		  					Step_4 := true;
		  					actStep_4 := true;
		  					deactStep_4 := false;
		  				END_IF
		  				(****** T_6 ******)
		  				IF ((DI_6)AND(T_6)) THEN
		  					StableState := true;
		  					(****** deactivate_presteps ******)
		  					Step_3 := false;
		  					actStep_3 := false;
		  					deactStep_3 := true;
		  					(****** activate_poststeps ******)
		  					Step_4 := true;
		  					actStep_4 := true;
		  					deactStep_4 := false;
		  				END_IF
		  				(****** T_5 ******)
		  				IF ((DI_5)AND(T_5)) THEN
		  					StableState := true;
		  					(****** deactivate_presteps ******)
		  					Step_4 := false;
		  					actStep_4 := false;
		  					deactStep_4 := true;
		  					(****** activate_poststeps ******)
		  					Step_1 := true;
		  					actStep_1 := true;
		  					deactStep_1 := false;
		  				END_IF
		  				(****** T_4 ******)
		  				IF ((DI_4)AND(T_4)) THEN
		  					StableState := true;
		  					(****** deactivate_presteps ******)
		  					Step_1 := false;
		  					actStep_1 := false;
		  					deactStep_1 := true;
		  					(****** activate_poststeps ******)
		  					Step_3 := true;
		  					actStep_3 := true;
		  					deactStep_3 := false;
		  				END_IF
		  			END_IF
		  			(****** Step_Part ******)
		  			(****** Step_1 ******)
		  			IF (actStep_1)OR(Step_1)OR(deactStep_1) THEN
		  				IF actStep_1 THEN
		  					actStep_1 := false;
		  				END_IF
		  				IF Step_1 THEN
		  					;
		  				END_IF
		  				IF deactStep_1 THEN
		  					deactStep_1 := false;
		  				END_IF
		  			END_IF
		  			(****** Step_5 ******)
		  			IF (actStep_5)OR(Step_5)OR(deactStep_5) THEN
		  				IF actStep_5 THEN
		  					actStep_5 := false;
		  				END_IF
		  				IF Step_5 THEN
		  					;
		  				END_IF
		  				IF deactStep_5 THEN
		  					deactStep_5 := false;
		  					AO_1  :=  (AO_1+1);
		  				END_IF
		  			END_IF
		  			(****** Step_4 ******)
		  			IF (actStep_4)OR(Step_4)OR(deactStep_4) THEN
		  				IF actStep_4 THEN
		  					actStep_4 := false;
		  				END_IF
		  				IF Step_4 THEN
		  					;
		  					DO_1 := true;
		  				END_IF
		  				IF deactStep_4 THEN
		  					deactStep_4 := false;
		  					DO_1 := false;
		  				END_IF
		  			END_IF
		  			(****** Step_3 ******)
		  			IF (actStep_3)OR(Step_3)OR(deactStep_3) THEN
		  				IF actStep_3 THEN
		  					actStep_3 := false;
		  				END_IF
		  				IF Step_3 THEN
		  					;
		  					DO_31 := true;
		  				END_IF
		  				IF deactStep_3 THEN
		  					deactStep_3 := false;
		  					DO_31 := false;
		  				END_IF
		  			END_IF
		  			END_WHILE;
		  		END_IF
		  		
