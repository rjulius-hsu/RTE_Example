    </vars>
    <vars name="actStep_3">
      <type xsi:type="plccode:BOOL"/>
      <initialValue value="false"/>
    </vars>
    <vars name="deactStep_3">
      <type xsi:type="plccode:BOOL"/>
      <initialValue value="false"/>
    </vars>
    <vars name="Step_3">
      <type xsi:type="plccode:BOOL"/>
      <initialValue value="false"/>
    </vars>
  </globalVars>
  <globalVars name="Global_GrafcetParts">
    <vars name="Global_Grafcet">
      <type xsi:type="plccode:Derived" name="Grafcet_Control"/>
    </vars>
  </globalVars>
</plccode:Project>
